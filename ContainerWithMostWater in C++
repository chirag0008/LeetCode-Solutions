int maxArea(vector<int>& height) {
        int n=height.size();
        int i=0;int j=n-1;
        int area;
        int max=INT_MIN;
        while(i<j){
            if(height[i]<=height[j]){
                area=height[i]*(j-i);
                i++;
            }
            else{
                area=height[j]*(j-i);
                j--;
            }
            if(max<area){
                max=area;
            }
        }
        return max;
        
    }
